/*		ArrayList list = new ArrayList();
		list.add(1);
		list.add(2);
		list.add(5);
		list.add(7);
		printDouble(list);*/
		// Starting fr Java 1.5
		ArrayList <Integer> list2 = new ArrayList();
		list2.add(1);
		list2.add(2);
		list2.add(5);
