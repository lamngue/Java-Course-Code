package com.vietjack;

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String info = "";
		info += "Toi ";
		info += "ten ";
		info += "la ";
		info += "Lam";
		System.out.println(info);
		StringBuilder sb = new StringBuilder("");
		sb.append("Toi ");
		sb.append("ten ");
		sb.append("la ");
		sb.append("Lam ");
		System.out.println(sb.toString());
	}

}
